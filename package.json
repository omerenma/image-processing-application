{
    "name": "image-processing-application",
    "version": "1.0.0",
    "description": "An image procession application that sclaes and built with typescript, express and jasmine for automation testing",
    "main": "server.ts",
    "scripts": {
        "build": "npx tsc",
        "dev": "tsc --watch",
        "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
        "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "test:init": "jasmine",
        "test": "yarn run build && yarn run jasmine",
        "start": "nodemon server.ts"
    },
    "author": "Kingsley Onyebuchi",
    "license": "ISC",
    "devDependencies": {
        "@types/eslint": "^8.4.10",
        "@types/express": "^4.17.15",
        "@types/file-type": "^10.9.1",
        "@types/jasmine": "^4.3.1",
        "@types/node": "^18.11.10",
        "@types/request": "^2.48.8",
        "@types/sharp": "^0.31.0",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.47.0",
        "@typescript-eslint/parser": "^5.47.0",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "2.8.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "all": "^0.0.0",
        "file-type": "^18.0.0",
        "jasmine": "^4.5.0",
        "jasmine-spec-reporter": "^7.0.0",
        "jasmine-supertest": "^1.0.0",
        "nodemon": "^2.0.20",
        "request": "^2.88.2",
        "sharp": "^0.31.3",
        "supertest": "^6.3.3"
    }
}
